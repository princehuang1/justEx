app.get("/api/concessions", (req, res) => {
  const sql = "SELECT * FROM Concessions";
  db.all(sql, [], (err, rows) => {
    if (err) {
      res.status(500).json({ error: err.message });
      return;
    }
    res.json(rows);
  });
});

